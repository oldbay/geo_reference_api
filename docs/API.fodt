   <text:p text:style-name="P1"><text:s text:c="12"/>&quot;test01&quot;</text:p>
   <text:p text:style-name="P1"><text:s text:c="8"/>],</text:p>
   <text:p text:style-name="P1"><text:s text:c="8"/>&quot;module_doc&quot;:&quot;\n <text:s text:c="3"/>Constructor for inhertance class ApiModule\n <text:s text:c="3"/>&quot;,</text:p>
   <text:p text:style-name="P1"><text:s text:c="8"/>&quot;resources&quot;:{</text:p>
   <text:p text:style-name="P1"><text:s text:c="12"/>&quot;table11&quot;:{</text:p>
   <text:p text:style-name="P1"><text:s text:c="16"/>&quot;resource_doc&quot;:&quot;\n <text:s text:c="3"/>test 11 table\n <text:s text:c="3"/>&quot;</text:p>
   <text:p text:style-name="P1"><text:s text:c="12"/>}</text:p>
   <text:p text:style-name="P1"><text:s text:c="8"/>}</text:p>
   <text:p text:style-name="P1"><text:s text:c="4"/>},</text:p>
   <text:p text:style-name="P1"><text:s text:c="4"/>&quot;test12&quot;:{</text:p>
   <text:p text:style-name="P1"><text:s text:c="8"/>&quot;module_depends&quot;:[</text:p>
   <text:p text:style-name="P1"><text:s text:c="12"/>&quot;test01&quot;</text:p>
   <text:p text:style-name="P1"><text:s text:c="8"/>],</text:p>
   <text:p text:style-name="P1"><text:s text:c="8"/>&quot;module_doc&quot;:&quot;\n <text:s text:c="3"/>Constructor for inhertance class ApiModule\n <text:s text:c="3"/>&quot;,</text:p>
   <text:p text:style-name="P1"><text:s text:c="8"/>&quot;resources&quot;:{</text:p>
   <text:p text:style-name="P1"><text:s text:c="12"/>&quot;table12&quot;:{</text:p>
   <text:p text:style-name="P1"><text:s text:c="16"/>&quot;resource_doc&quot;:&quot;\n <text:s text:c="3"/>test 12 table\n <text:s text:c="3"/>&quot;</text:p>
   <text:p text:style-name="P1"><text:s text:c="12"/>}</text:p>
   <text:p text:style-name="P1"><text:s text:c="8"/>}</text:p>
   <text:p text:style-name="P1"><text:s text:c="4"/>}</text:p>
